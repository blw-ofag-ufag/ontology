    "http://purl.org/dc/terms/title": [
      {
        "@value": "A (demonstrative) ontology for crop protection in Switzerland",
        "@language": "en"
      },
      {
        "@value": "Eine (demonstrative) Ontologie für den Pflanzenschutz in der Schweiz",
        "@language": "de"
      }
    ],
    "http://purl.org/dc/terms/description": [
      {
        "@value": "This ontology exemplifies the value of comprehensive and well-structured ontology development for the Swiss Federal Office for Agriculture (and more broadly the Swiss agri-food sector). By capturing the complexities of crop protection (with a focus on registered products) in a formal model, it enhances understanding of current rules and allows their effective communication via IT-services.",
        "@language": "en"
      },
      {
        "@value": "Diese Ontologie veranschaulicht den Wert einer umfassenden und gut strukturierten Ontologieentwicklung für das Bundesamt für Landwirtschaft (und allgemein den Schweizer Agrar- und Lebensmittelsektor). Durch die formale Modellierung der Komplexität des Pflanzenschutzes (mit Schwerpunkt auf zugelassenen Produkten) verbessert sie das Verständnis der aktuellen Regeln und ermöglicht deren effektive Kommunikation über IT-Dienste.",
        "@language": "de"
      }
    ],
    "http://www.w3.org/2002/07/owl#versionInfo": [
      {
        "@value": "0.6"
      }
    ],
    "http://purl.org/dc/terms/creator": [
      {
        "@value": "Damian Oswald"
      }
    ]
  },
